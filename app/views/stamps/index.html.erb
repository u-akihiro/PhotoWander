<h1>Listing stamps</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>File type</th>
      <th>File size</th>
      <th>File width</th>
      <th>File height</th>
      <th>Checkpoint</th>
      <th>Entry card</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @stamps.each do |stamp| %>
      <tr>
        <td><%= stamp.file_name %></td>
        <td><%= stamp.file_type %></td>
        <td><%= stamp.file_size %></td>
        <td><%= stamp.file_width %></td>
        <td><%= stamp.file_height %></td>
        <td><%= stamp.checkpoint_id %></td>
        <td><%= stamp.entry_card_id %></td>
        <td><%= link_to 'Show', stamp %></td>
        <td><%= link_to 'Edit', edit_stamp_path(stamp) %></td>
        <td><%= link_to 'Destroy', stamp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stamp', new_stamp_path %>
